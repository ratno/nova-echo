{
    "name": "ratno/nova-echo",
    "description": "Adds Laravel Echo with your broadcast configuration to your Laravel Nova app",
    "keywords": [
        "laravel",
        "nova",
        "echo",
        "websockets",
        "socket-io",
        "broadcasting",
        "real-time"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ratno\\NovaEcho\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ratno\\NovaEcho\\NovaEchoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
